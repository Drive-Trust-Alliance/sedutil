        "version": "0.2.0",
        "configurations": [

          {
            "name": "sedutil_debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "sedutil-cli",
            "args": ["--query","/dev/sda"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
              {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
              }
            ],
            "preLaunchTask": "sedutil_build",
            "miDebuggerPath": "/usr/bin/gdb"
          }
        ]
      }